{% extends 'Template.html.twig' %}
    {% block title %}Ajout Demande Coursier{% endblock %}

{% block body %}


    {{ form_start(form, {'method': 'post', 'action': path('_create_demande_coursier'), 'attr': {'class': ''}}) }}

    {{ form_errors(form) }}



    <div class="col-md-6">
    <div class="box box-primary">




    <div class="box-header with-border">
        <h3 class="box-title">Ajout demande coursier</h3>
    </div>
    <!-- /.box-header -->
    <!-- form start -->
    <div class="box-body">

    <div class="form-group">
        <label for="exampleInputEmail1">Quoi :</label>
        {{ form_row(form.quoi) }}
    </div>


        <div class="form-group">
            <label for="exampleInputEmail1">Destinataire :</label>
            {{ form_row(form.nomPrenomRecept) }}
        </div>


        <div class="form-group">
            <label for="exampleInputEmail1">Lieu :</label>
            {{ form_row(form.addresseRecept) }}
        </div>

        <div class="form-group">
            <label for="exampleInputEmail1">Type :</label>
            {{ form_row(form.typeDC) }}
        </div>


        <div class="form-group">
            <label for="exampleInputEmail1">Etat :</label>
            {{ form_row(form.etat) }}
        </div>



        <center>    <button type="submit" class="btn btn-lg btn-primary">Envoyer</button>
        </center>

    </div></div></div>







    {{ form_rest(form) }}
    {{ form_end(form) }}

{% endblock %}